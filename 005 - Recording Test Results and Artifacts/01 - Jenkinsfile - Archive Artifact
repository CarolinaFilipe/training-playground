pipeline {
    agent { 
		dockerfile { 
			dir '005 - Recording Test Results and Artifacts'
			filename '01 - Docker - Jenkinsfile - Archive Artifact'
		}
	}
    stages {
        stage('build') {
            steps {
                sh "curl www.google.com > googleCurl.html"
            }
        }
    }
    post {
    	success {
		archiveatrifacts artifacts 'googleCurl.html'
	}
    }
}
