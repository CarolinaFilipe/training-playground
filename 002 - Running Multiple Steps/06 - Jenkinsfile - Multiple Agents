pipeline {
    agent none
    
    stages {
    	stage ('buildmvn'){
		agent {
			docker {
				image 'mvn:3.3.3'
			}
		}
    		steps {
			sh "mvn --version"
		}
	}
	stage ('buildnpm'){
		agent {
			docker {
				image 'node:6.3'
			}
		}
    		steps {
			sh "npm--version"
		}
	}
    }
}
