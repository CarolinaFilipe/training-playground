pipeline {
    agent none
    
    stages {
    	stage ('buildmvn'){
		agent {
			dockerfile {
				dir '003 - Defining Execution Environments'
				filename '04 - DockerFilie - Multiple Agents'
				
			}
		}
    		steps {
			sh "mvn --version"
		}
	}
	stage ('buildnpm'){
		agent {
			docker {
				image 'node:6.3'
			}
		}
    		steps {
			sh "npm --version"
		}
	}
    }
}
