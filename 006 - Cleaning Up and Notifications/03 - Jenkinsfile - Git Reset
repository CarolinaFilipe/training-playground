pipeline {
    agent { 
		dockerfile { 
			 filename '03 - Jenkinsfile - Git Image'
        		 dir '006 - Cleaning Up and Notifications/03 - Jenkinsfile - Git Reset' 
		}
	}
    stages {
        stage('build') {
            steps {
	    	sh "ls -lta"
		sh "git clean -fd"
		sh "ls -lta"
                sh "date > date.txt"
		sh "ls -lta"
		
            }
        }
    }
}
